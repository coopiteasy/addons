<odoo>
    <record id="cie_fix_account_analytic_line_so_line" model="ir.cron">
        <field name="name">cie fix account_analytic_line so_line</field>
        <field name="model_id" ref="model_account_analytic_line" />
        <field name="active" eval="False" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field> <!-- don't limit the number of calls -->
        <field name="doall" eval="False" />
        <field name="code">model.reset_so_line()</field>
        <field name="args" eval="'()'" />
    </record>

    <record id="cie_compute_aaa_line_count_cron" model="ir.cron">
        <field name="name">Compute line on analytic account (over last month)</field>
        <field name="model_id" ref="model_account_analytic_account" />
        <field name="active" eval="True" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field> <!-- don't limit the number of calls -->
        <field name="doall" eval="False" />
        <field name="code">model.cron_compute_line_count()</field>
        <field name="args">()</field>
    </record>
</odoo>
