<?xml version="1.0" ?>
<odoo>
    <record id="view_event_track_form" model="ir.ui.view">
        <field name="model">event.track</field>
        <field name="inherit_id" ref="website_event_track.view_event_track_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="books" string="Books">
                    <field name="book_ids">
                        <tree>
                            <field name="name" />
                            <field name="author_ids" widget="many2many_tags" />
                            <field name="editor_id" />
                            <field name="speaker_ids" widget="many2many_tags" />
                            <field name="user_id" />
                            <field name="status" />
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
