    name="product_label_report.report_same_product_label_10x4"
    file="product_label_report.same_product_label.xml"/>

  <report
    id="product_template_label_10x4"
    string="Product Labels 10x4"
    model="product.template"
    report_type="qweb-pdf"
    paperformat="paperformat_a4_nomargin"
    name="product_label_report.report_product_label_10x4"
    file="product_label_report.product_label.xml"/>

  <report
    id="product_template_label_repeat_13x5"
