        <field name="res_model">event.track.speaker.travel.expense</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="action_travel_booking">
        <field name="name">Travel Bookings</field>
        <field name="res_model">event.track.speaker.travel.booking</field>
